{
  "name": "vibe-check-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "tsc && echo 'TypeScript compilation complete'",
    "dev": "tsx watch src/index.ts",
    "reset-db": "rm -f prisma/dev.db && npx prisma migrate reset --force",
    "update-prompts": "npx tsx scripts/update-prompts.ts",
    "seed": "npx prisma db seed",
    "reset-votes": "npx prisma db execute --file ./prisma/reset-votes.sql",
    "reset-all": "npx prisma migrate reset --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "openai": "^4.24.1",
    "prisma": "^6.2.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0"
  },
  "description": "",
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  }
}
